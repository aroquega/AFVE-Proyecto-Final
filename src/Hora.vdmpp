class Hora

types
	public Horas = nat1;
	public Minutos = nat1;
	public Segundos = nat1;

instance variables
	public horas : Horas :=1;
	public minutos : Minutos := 1;
	public segundos : Segundos := 1;
	inv horas <= 24 and minutos <= 60 and segundos <= 60;
	
operations 
 
	public Hora: Horas * Minutos * Segundos ==> Hora
	Hora(h, m, s) == (horas := h;
										minutos := m;
										segundos := s);

end Hora