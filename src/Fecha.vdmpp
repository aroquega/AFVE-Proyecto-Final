class Fecha

types
--public dia = nat1;
--public mes = nat1;
--public axo = int;

instance variables
	public dia : nat1 :=1;
	public mes : nat1 := 1;
	inv dia <= 31 and mes <= 12 and 
		if mes in set {4,9,6,11}
		then dia <= 30
		else (mes = 2) => (dia <= 29);
	
	public axo : int;

operations
	public Fecha: nat1 * nat1 * int ==> Fecha
	 Fecha(d,m,a) == (dia := d;
	   								mes := m;
	   								axo := a);

end Fecha